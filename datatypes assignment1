#include <iostream>

using namespace std;

int main()
{
    char a;
    int b;
    float c;
    double d;

    cout << "Size of char is: "<< sizeof(a)<<endl;
    cout << "size of int is: "<< sizeof(b)<<endl;
    cout << "size of float is: "<<sizeof(c)<<endl;
    cout << "size of double is: "<<sizeof(d);
    return 0;
}
